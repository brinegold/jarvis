{
  "name": "jarvis-staking-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "build:analyze": "npm run build && npm run analyze",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "perf:unix": "npm run build && npm run start & sleep 5 && npm run lighthouse",
    "perf:win": "npm run build && start /b npm run start && timeout /t 5 && npm run lighthouse",
    "perf": "npm run perf:win",
    "test:build": "node scripts/test-build.js",
    "filter:usdt": "node scripts/filter_usdt_wallets.js scripts/sample_addresses.json",
    "filter:bnb": "node scripts/filter_bnb_wallets.js scripts/sample_addresses.json",
    "collect:usdt": "ts-node --project tsconfig.scripts.json scripts/collect_usdt_from_wallets.ts",
    "collect:bnb": "ts-node --project tsconfig.scripts.json scripts/collect_bnb_from_wallets.ts"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/auth-helpers-react": "^0.4.2",
    "@supabase/supabase-js": "^2.38.4",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "next": "14.0.3",
    "nodemailer": "^6.10.1",
    "postcss": "^8.4.32",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^4.12.0",
    "recharts": "^2.8.0",
    "swr": "^2.3.6",
    "tailwindcss": "^3.3.6",
    "web3": "^4.16.0",
    "web3-eth-contract": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.23",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "critters": "^0.0.20",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "lighthouse": "^11.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
